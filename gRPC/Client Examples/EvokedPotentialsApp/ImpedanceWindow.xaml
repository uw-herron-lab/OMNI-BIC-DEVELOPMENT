<Window x:Class="EvokedPotentialsApp.ImpedanceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EvokedPotentialsApp"
        mc:Ignorable="d"
        Title="ImpedanceWindow" Height="450" Width="800">
    <Grid>
        <ScrollViewer Name="impScroller" Margin="0,0.4,-0.4,0" Background="White" Grid.ColumnSpan="3" VerticalScrollBarVisibility="Auto" Grid.Row="1">
            <TextBlock Name="ImpedanceOutputConsole" TextWrapping="Wrap" Foreground="Black" FontFamily="Consolas" FontSize="15"/>
        </ScrollViewer>
    </Grid>
</Window>
